<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SettingItem">
        <attr name="setting_openable" format="boolean" />
        <attr name="setting_has_switch" format="boolean" />
        <attr name="setting_name" format="string" />
        <attr name="setting_value" format="string" />
        <attr name="setting_image" format="reference" />
    </declare-styleable>

    <declare-styleable name="EndpointItem">
        <attr name="endpoint_name" format="string" />
        <attr name="endpoint_value" format="string" />
    </declare-styleable>

    <declare-styleable name="LabeledValueItem">
        <attr name="item_label" format="string" />
    </declare-styleable>

    <declare-styleable name="AddressItem">
        <attr name="show_address_separator" format="boolean" />
    </declare-styleable>

    <declare-styleable name="NamedAddressItem">
        <attr name="show_named_address_separator" format="boolean" />
    </declare-styleable>
</resources>
